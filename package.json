{
  "name": "nuktatestify",
  "version": "1.0.2",
  "description": "Generate comprehensive, production-ready Jest test cases for Express.js APIs with MongoDB support",
  "main": "dist/index.js",
  "bin": {
    "nuktatestify": "dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "jest",
    "express",
    "test",
    "cli",
    "generator",
    "automation",
    "mongodb",
    "typescript",
    "api",
    "testing",
    "integration",
    "authentication",
    "validation",
    "performance"
  ],
  "author": "nurktadev",
  "license": "MIT",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.8.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.7",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.0.0",
    "@types/node": "^24.0.15",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuktadev/nuktatestify.git"
  },
  "bugs": {
    "url": "https://github.com/nuktadev/nuktatestify/issues"
  },
  "homepage": "https://github.com/nuktadev/nuktatestify#readme"
}
